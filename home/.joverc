define-macro rightmargin ^[Xset right-margin 72^M
define-macro settab4 ^[Xset tab-width 4^M
define-macro setindent4 ^[Xset c-indentation-increment 4^M
set allow-^S-and-^Q on
set bad-filename-extensions .o .ln
set c-indentation-increment 8
set expand-environment-variables on
#set error-format-string ^[^:]*:[^:]*: *\([^,]*\), *line *\([0-9][0-9]*\):
1 auto-execute-command auto-fill-mode /tmp/snd.*\|.*\.article\|.*\.letter\|/tmp/nn.*\|/usr/tmp/nn.*\|.*/drafts/.*
1 auto-execute-command show-match .*\.[lchy]$\|.*\.cpp$\|.*\.cc$\|.*\.lisp$\|.*\.scm$\|.*\.slisp\|.*\.pl$\|.*\.pm$\|.*\.java$\|.*\.php$
1 auto-execute-command c-mode .*\.[chy]$\|.*\.cpp$\|.*\.cc$\|.*\.pl$\|.*\.pm$\|.*\.java$\|.*\.php$
1 auto-execute-command lisp-mode .*\.l$\|.*\.lisp$\|.*\.scm$\|.*\.slisp
auto-execute-macro rightmargin .*/drafts/.*
auto-execute-macro settab4 .*\.[chy]$\|.*\.cpp$\|.*\.pl$\|.*\.java$\|.*\.php$
auto-execute-macro setindent4 .*\.[chy]$\|.*\.cpp$\|.*\.pl$\|.*\.java$\|.*\.php$
