bind-to-key i-search-forward ^S
bind-to-key i-search-reverse ^R
bind-to-key query-replace-string ^[%
bind-to-key make-buffer-unmodified ^[*
bind-to-key filter-region ^X|
bind-to-key i-shell-command ^[!
bind-to-key fill-comment ^Xj
bind-to-key fill-comment ^XJ
bind-to-key buffer-position ^X=
bind-to-key kill-previous-word ^[^H
bind-to-key find-tag-at-point ^Xt
bind-to-key find-tag-at-point ^XT
